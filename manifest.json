{
  "name": "FrontCat",
  "description": "오히려 좋아",
  "version": "1.0.1",
  "manifest_version": 3,
  "author": "cheo (https://github.com/datalater)",
  "contributers": ["Jonghyeon Ko (https://github.com/manudeli)"],
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/extension_icon16px.png",
      "32": "/images/extension_icon32px.png",
      "48": "/images/extension_icon48px.png",
      "128": "/images/extension_icon128px.png"
    }
  },
  "icons": {
    "16": "/images/extension_icon16px.png",
    "32": "/images/extension_icon32px.png",
    "48": "/images/extension_icon48px.png",
    "128": "/images/extension_icon128px.png"
  }
}
